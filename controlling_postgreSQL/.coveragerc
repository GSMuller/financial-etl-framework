[run]
source = .
omit = 
    tests/*
    */__pycache__/*
    */site-packages/*
    */.venv/*
    */venv/*
    */env/*
    setup.py

[report]
precision = 2
show_missing = True
skip_covered = False
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:

[html]
directory = htmlcov
